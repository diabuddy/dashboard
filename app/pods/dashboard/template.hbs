<div id='dashboard' class="toLeft">
  {{banner-card selectedRange=selectedRange selectedBuddies=selectedBuddies class='card banner bs'}} {{!-- This is where our data cards go --}}
  <div class="charts layout-row">
    <div class='card card-blue chart'>
      <div class='header'>
        Insulin Chart Plot
      </div>
      <div class='content'>
        {{insulin-shot-plot class='dashboard-card'}}
      </div>
    </div>
    <div class='chart card card-red'>
      <div class='header'>
        Blood Sugar Plot
      </div>
      <div class='content'>
        {{blood-sugar-plot class='dashboard-card'}}
      </div>
    </div>
  </div>

  <div class="charts layout-row">
    <div class='chart card card-orange'>
      <div class='header'>
        Reward Board
      </div>
      <div class='content'>
        {{score-board class='dashboard-card'}}
      </div>
    </div>
  </div>
  {{userData}}
  {{outlet}}

</div>